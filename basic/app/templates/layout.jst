
<div class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <ul class="nav">
        <% _.each(pages, function(page) { %>
          <% if (page == cur_page) { %>
            <li class="active">
          <% } else { %>
            <li>
          <% } %>
            <a href="<%= url_root %>#<%= page %>"><%= page %></a>
          </li>
        <% }); %>
      </ul>
    </div>
  </div>
</div>

<div class="container">
  <div class="content"></div>
</div>
